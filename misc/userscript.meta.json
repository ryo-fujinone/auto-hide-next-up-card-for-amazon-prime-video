{
  "name": "Auto hide next up card for Amazon Prime Video",
  "namespace": "http://tampermonkey.net/",
  "version": null,
  "description": "Hide next up card and other obtrusive elements of Amazon Prime Video.",
  "author": "ryo-fujinone",
  "match": [
    "https://*.amazon.co.jp/*",
    "https://*.amazon.com/*",
    "https://*.amazon.ae/*",
    "https://*.amazon.co.uk/*",
    "https://*.amazon.it/*",
    "https://*.amazon.in/*",
    "https://*.amazon.eg/*",
    "https://*.amazon.com.au/*",
    "https://*.amazon.nl/*",
    "https://*.amazon.ca/*",
    "https://*.amazon.sa/*",
    "https://*.amazon.sg/*",
    "https://*.amazon.se/*",
    "https://*.amazon.es/*",
    "https://*.amazon.de/*",
    "https://*.amazon.com.tr/*",
    "https://*.amazon.com.br/*",
    "https://*.amazon.fr/*",
    "https://*.amazon.com.be/*",
    "https://*.amazon.pl/*",
    "https://*.amazon.com.mx/*",
    "https://*.amazon.cn/*",
    "https://*.primevideo.com/*"
  ],
  "grant": [
    "GM_registerMenuCommand",
    "GM.registerMenuCommand ",
    "GM_getResourceText"
  ],
  "resource": {
    "xhook": "https://cdn.jsdelivr.net/gh/ryo-fujinone/auto-hide-next-up-card-for-amazon-prime-video@78d9834a5e04046a2b75ed8777d88faf3d106064/src/xhook.min.js"
  },
  "license": "MIT; https://github.com/ryo-fujinone/auto-hide-next-up-card-for-amazon-prime-video/blob/main/LICENSE"
}
